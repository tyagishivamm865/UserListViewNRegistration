<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imgcancel"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="8dp"
        android:src="@drawable/baseline_cancel_24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.954"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:background="@color/black"
        android:layout_marginTop="5dp"
        app:layout_constraintTop_toBottomOf="@id/imgcancel"
        app:layout_constraintStart_toStartOf="parent"

        />

    <TextView
        android:id="@+id/txtbasic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Basic Info"
        android:textColor="@color/black"
        android:textSize="17sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="105dp"
        app:layout_constraintTop_toBottomOf="@+id/view"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/imgcircular"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_margin="15dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/profile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.045"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtbasic" />

    <TextView
        android:id="@+id/txtemail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="enter@gmail.com"
        android:textSize="15sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="105dp"
        app:layout_constraintTop_toBottomOf="@+id/imgcircular"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="15dp"
        android:layout_marginTop="5dp" />

    <TextView
        android:id="@+id/txtphone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="9999999999"
        android:textSize="15sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="105dp"
        app:layout_constraintTop_toBottomOf="@+id/txtemail"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="15dp"
        android:layout_marginTop="5dp" />

    <TextView
        android:id="@+id/txtfullname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="FUll Name"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/view"
        app:layout_constraintStart_toStartOf="@id/imgcircular"
        android:layout_marginStart="130dp"
        android:layout_marginTop="40dp" />

    <TextView
        android:id="@+id/txtdesignation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Designation"
        android:textColor="@color/black"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/txtfullname"
        app:layout_constraintStart_toStartOf="@id/imgcircular"
        android:layout_marginStart="130dp"
        android:layout_marginTop="10dp" />

    <TextView
        android:id="@+id/txtAddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Address"
        android:textColor="@color/black"
        android:textSize="15sp"
        app:layout_constraintTop_toBottomOf="@+id/txtdesignation"
        app:layout_constraintStart_toStartOf="@id/imgcircular"
        android:layout_marginStart="130dp"
        android:layout_marginTop="8dp" />

    <TextView
        android:id="@+id/txtlandmark"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Landmark"
        android:textColor="@color/black"
        android:textSize="15sp"
        app:layout_constraintTop_toBottomOf="@+id/txtAddress"
        app:layout_constraintStart_toStartOf="@id/imgcircular"
        android:layout_marginStart="130dp"/>

    <TextView
        android:id="@+id/txtcitystate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="city"
        android:textColor="@color/black"
        android:textSize="15sp"
        app:layout_constraintTop_toBottomOf="@+id/txtlandmark"
        app:layout_constraintStart_toStartOf="@id/imgcircular"
        android:layout_marginStart="130dp"/>

    <TextView
        android:id="@+id/txtpin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="pincode"
        android:textColor="@color/black"
        android:textSize="15sp"
        app:layout_constraintTop_toBottomOf="@+id/txtcitystate"
        app:layout_constraintStart_toStartOf="@id/imgcircular"
        android:layout_marginStart="130dp"/>

    <View
        android:id="@+id/view2"
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:background="@color/black"
        android:layout_marginTop="12dp"
        app:layout_constraintTop_toBottomOf="@id/txtpin"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_margin="15dp"
        />

    <TextView
        android:id="@+id/txteducationinfo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Educational Info"
        android:textColor="@color/black"
        android:textSize="17sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="105dp"
        app:layout_constraintTop_toBottomOf="@+id/view2"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="15dp"
        android:layout_marginTop="12dp" />

    <TextView
        android:id="@+id/txteducation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Graduate"
        android:textSize="15sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="105dp"
        app:layout_constraintTop_toBottomOf="@+id/txteducationinfo"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="15dp"
        android:layout_marginTop="5dp" />

    <TextView
        android:id="@+id/txtyearofpassing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Passing year - 2014"
        android:textSize="15sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="105dp"
        app:layout_constraintTop_toBottomOf="@+id/txteducation"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="15dp"/>

    <TextView
        android:id="@+id/txtcgpa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cgpa-9.5"
        android:textSize="15sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="105dp"
        app:layout_constraintTop_toBottomOf="@+id/txtyearofpassing"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="15dp"/>

    <View
        android:id="@+id/view3"
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:background="@color/black"
        android:layout_marginTop="12dp"
        app:layout_constraintTop_toBottomOf="@id/txtcgpa"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_margin="15dp"
        />

    <TextView
        android:id="@+id/txtProfessionalinfo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Professional Info"
        android:textColor="@color/black"
        android:textSize="17sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="105dp"
        app:layout_constraintTop_toBottomOf="@+id/view3"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="15dp"
        android:layout_marginTop="15dp" />

    <TextView
        android:id="@+id/txtyearofexperience"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="experience"
        android:textSize="15sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="105dp"
        app:layout_constraintTop_toBottomOf="@+id/txtProfessionalinfo"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="15dp"
        android:layout_marginTop="5dp" />

    <TextView
        android:id="@+id/txtdomain"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="Passing year - 2014"
        android:textSize="15sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="105dp"
        app:layout_constraintTop_toBottomOf="@+id/txtyearofexperience"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="15dp"
        android:layout_marginTop="5dp"
        />

</androidx.constraintlayout.widget.ConstraintLayout>